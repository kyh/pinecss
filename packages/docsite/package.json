{
  "private": true,
  "name": "pinnecss-docsite",
  "version": "0.0.3",
  "keywords": [],
  "description": "Docs for PineCSS",
  "repository": {
    "type": "git",
    "url": "https://github.com/kyh/pinecss",
    "directory": "packages/docsite"
  },
  "author": "Kaiyu Hsu @kyh",
  "license": "MIT",
  "scripts": {
    "build": "npm run build:css && npm run build:remix",
    "build:css": "tailwindcss -i ./styles/global.css -o ./app/tailwind.css --minify",
    "build:remix": "remix build",
    "dev": "concurrently \"npm:dev:*\"",
    "dev:css": "tailwindcss -i ./styles/global.css -o ./app/tailwind.css --watch",
    "dev:remix": "remix dev",
    "postinstall": "remix setup node"
  },
  "dependencies": {
    "@headlessui/react": "^1.4.3",
    "@heroicons/react": "^1.0.5",
    "@remix-run/react": "^1.1.3",
    "@remix-run/serve": "^1.1.3",
    "@remix-run/vercel": "^1.1.3",
    "classnames": "^2.3.1",
    "pinecss": "^0.0.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.1.3"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.1.3",
    "@tailwindcss/typography": "^0.5.0",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "tailwindcss": "^3.0.15",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
