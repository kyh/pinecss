{
  "name": "breezecss",
  "version": "0.0.1",
  "keywords": [],
  "description": "",
  "style": "css/breeze.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/kyh/breeze",
    "directory": "packages/breezecss"
  },
  "author": "Kaiyu Hsu @kyh",
  "license": "MIT",
  "files": [
    "css"
  ],
  "scripts": {
    "dev": "npm run build:dev -- --watch --verbose",
    "prebuild": "npm run clean",
    "build": "concurrently \"npm run build:dev\" \"npm run build:prod\"",
    "build:dev": "postcss ./src/breeze.css -o ./css/breeze.css",
    "build:prod": "NODE_ENV=production postcss ./src/breeze.css -o ./css/breeze.min.css --no-map",
    "prepublish": "npm run build",
    "clean": "rimraf ./css"
  },
  "dependencies": {},
  "devDependencies": {
    "breezecss-preset": "^0.0.1",
    "autoprefixer": "^10.4.0",
    "cssnano": "^5.0.13",
    "postcss": "^8.4.5",
    "postcss-cli": "^9.1.0",
    "postcss-import": "^14.0.2",
    "tailwindcss": "^3.0.7"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
